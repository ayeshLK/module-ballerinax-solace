<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Suppress EI_EXPOSE_REP and EI_EXPOSE_REP2 for SecureSocketConfig -->
    <!-- The List fields use List.of() which returns unmodifiable lists, so these warnings are false positives -->
    <Match>
        <Class name="io.ballerina.lib.solace.config.ssl.SecureSocketConfig"/>
        <Or>
            <Bug pattern="EI_EXPOSE_REP"/>
            <Bug pattern="EI_EXPOSE_REP2"/>
        </Or>
    </Match>
</FindBugsFilter>
